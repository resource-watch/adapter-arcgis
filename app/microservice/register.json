{
  "name": "Arcgis adapter",
  "cache": [
    "query-arcgis"
  ],
  "uncache": [
    "query-arcgis"
  ],
  "tags": [
    "rw"
  ],
  "endpoints": [
    {
      "path": "/v1/query/:dataset",
      "method": "GET",
      "binary": true,
      "redirect": {
        "method": "POST",
        "path": "/api/v1/arcgis/query/:dataset"
      },
      "filters": [
        {
          "name": "dataset",
          "path": "/v1/dataset/:dataset",
          "method": "GET",
          "params": {
            "dataset": "dataset"
          },
          "compare": {
            "data": {
              "attributes": {
                "provider": "featureservice"
              }
            }
          }
        }
      ]
    },
